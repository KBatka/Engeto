#rozpracováno

my_new_dict = {'m': 12345, 'n': 32145, 'o': 54321, 'p': 23232, 'q': 43210, 'r': 13579}

maximal_value_of_key = max(my_new_dict.keys())
print("max value of key:" + maximal_value_of_key)

maximal_value = max(my_new_dict.values())
print("max value:", maximal_value)

if maximal_value > maximal_value_of_key:
    print("hurrá")

print(my_new_dict)
